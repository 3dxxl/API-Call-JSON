<!--Bild fetch-->

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Static Template</title>
  </head>
  <body>
    <h1>Fetch an Image</h1>
    <img src="" id="rainbow" width="480" />
    <script>
      console.log("Es geht los");

      catchRainbow()
        .then(response => {
          console.log("yay");
        })
        .catch(error => {
          console.log("Fehler!");
          console.error(Fehler);
        });

      async function catchRainbow() {
        const response = await fetch(
          "data:image/jpeg;base64,"
        );
        const blob = await response.blob();
        document.getElementById("rainbow").src = URL.createObjectURL(blob);
      }
    </script>
  </body>
</html>

-------------------

<!--API Call, fetch json u. pack es in den html tag mit Authorization token, header-->
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Static Template</title>
  </head>
  <body>
    <h1>Fetch API</h1>
    <p>Output:</br>
      <span id="note"></span>
    </p>
    <script>
      console.log("Es geht los");

      Asa()
        .then(response => {
          console.log("yay");
        })
        .catch(error => {
          console.log("Fehler!");
          console.error(Fehler);
        });

      async function Asa() {
        var bearer =
          "Bearer " + "AuthorizationToken z.b. 12321321/sdlkfj32131";
        const response = await fetch(
          "API Endpoint URL",
          {
            method: "get",
            headers: {
              Authorization: bearer,
              "Content-Type": "application/json"
            }
          }
        );
        const data = await response.json();
        console.log(data.data.notes);


        document.getElementById("note").textContent = data.data.notes;
      }
    </script>
  </body>
</html>


--------------
/**Einfacher API call mit React  */


import React, { Component } from "react";
import ReactDOM from "react-dom";

import "./styles.css";

export default class App extends Component {
  constructor(props) {
    super(props);

    this.state = {
      eins: "black",
      zwei: this.drei,
      dataString: "",
      dataArray: [],
    };
  }

  componentDidMount() {
    // Simple GET request using fetch, with an Array Endpoint
    fetch("https://jsonplaceholder.typicode.com/todos/")
      .then(response => response.json())
      .then(response => {
        console.log(response);
        this.setState({
          dataArray: response
                });
      });


       // Simple GET request using fetch, with an JSON Key/Value Basic Paire / Sting Endpoint
    fetch("https://jsonplaceholder.typicode.com/todos/1")
    .then(response => response.json())
    .then(response => {
      console.log(response.userId);
      this.setState({
        dataString: response.userId
      });
    });

  }

  render() {
    return (
      <div>
        <h1>Simple Fetch in React</h1>

        <p>{this.state.dataString}</p>
       
        <ul>
          {this.state.dataArray.map(function(item, index) {
            return <h1> {item.id} </h1>;
          })}
        </ul>
      
      </div>
    );
  }
}

const rootElement = document.getElementById("root");
ReactDOM.render(<App />, rootElement);


