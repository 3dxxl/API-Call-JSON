      <script>
  
      const jsapi_url = "https://jsonplaceholder.typicode.com/todos/1";
      async function getjsData() {
        const response = await fetch(jsapi_url);
        const data = await response.json();
        console.log(data);
        console.log(data.title);
      }

      getjsData();




